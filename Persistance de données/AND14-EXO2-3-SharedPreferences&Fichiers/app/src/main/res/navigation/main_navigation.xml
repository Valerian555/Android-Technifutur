<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.technipixl.exo2.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_loginFragment_to_logsFragment"
            app:destination="@id/logsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.technipixl.exo2.SettingsFragment"
        android:label="SettingsFragment" />
    <fragment
        android:id="@+id/logsFragment"
        android:name="com.technipixl.exo2.LogsFragment"
        android:label="fragment_logs"
        tools:layout="@layout/fragment_logs" />
</navigation>